{
  "buildCommand": "npm install && cd apps/web && npm run build",
  "outputDirectory": "apps/web/.next",
  "installCommand": "npm install --prefix apps/web",
  "devCommand": "cd apps/web && npm run dev",
  "framework": "nextjs",
  "ignoreCommand": "git diff --quiet $VERCEL_GIT_PREVIOUS_SHA $VERCEL_GIT_COMMIT_SHA -- apps/web"
}